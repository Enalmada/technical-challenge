{
  "name": "nextjs-stack-example",
  "version": "1.0.0",
  "scripts": {
    "analyze": "cross-env ANALYZE=true npm run buildLocal",
    "build": "next build",
    "buildLocal": "cross-env NODE_ENV=production next build",
    "check-types": "tsc",
    "check-types-watch": "npm run check-types -- --watch --preserveWatchOutput",
    "db-migrate-create": "node db-migrate-create.js",
    "db-migrate-down": "node db-migrate-down.js",
    "db-migrate-up": "node db-migrate-up.js",
    "db-migrate-up-prod": "env-cmd -f .env.prod node db-migrate-up.js",
    "dev": "next",
    "eslint-fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "generateTypes": "graphql-codegen --config graphql-codegen.yml",
    "postinstall": "prisma generate",
    "predev": "npm run db-migrate-up",
    "prestart": "npm run db-migrate-up",
    "prisma-generate": "npx @prisma/cli generate",
    "prisma-introspect": "cross-env DATABASE_URL=postgres://postgres:password@127.0.0.1:5433/stackdemofirebase prisma introspect",
    "serverlessDeployTest": "env-cmd -f .env.prod serverless",
    "start": "next start",
    "test": "npm run test:unit",
    "test-full": "npm run test && npm run test:e2e",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:ui": "env-cmd -f .env.local cross-env NODE_ENV=test majestic",
    "test:unit:update": "jest --updateSnapshot"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@hookform/resolvers": "^1.0.0",
    "@prisma/client": "^2.10.2",
    "@sentry/browser": "^5.27.2",
    "@sentry/integrations": "5.27.2",
    "@sentry/node": "^5.27.2",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-link-timeout": "^3.0.0",
    "apollo-server-micro": "^2.19.0",
    "apollo-upload-client": "^14.1.2",
    "date-fns": "^2.16.1",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.3.0",
    "graphql": "^15.4.0",
    "isomorphic-unfetch": "^3.1.0",
    "mjml": "^4.7.1",
    "next": "^10.0.0",
    "next-offline": "^5.0.2",
    "next-seo": "^4.15.0",
    "nookies": "^2.5.0",
    "react": "^17.0.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.10.1",
    "react-tooltip": "^4.2.10",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.0",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.7",
    "@next/bundle-analyzer": "^10.0.0",
    "@prisma/cli": "^2.10.2",
    "@sentry/webpack-plugin": "^1.13.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/dom": "^7.26.4",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/react": "^16.9.55",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.2",
    "babel-plugin-graphql-tag": "^3.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.5.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv-flow": "^3.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.2",
    "jest-dom": "^4.0.0",
    "lint-staged": "^10.5.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "serverless": "^2.10.0",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.5",
    "whatwg-fetch": "^3.4.1"
  },
  "author": "Adam Lane <enalmada@gmail.com>",
  "description": "A boilerplate for next.js apps",
  "license": "UNLICENSED",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "main": ""
}
